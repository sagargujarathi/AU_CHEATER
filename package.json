  "devDependencies": {
    "@types/bun": "latest",
    "@types/chrome": "^0.0.287",
    "tailwindcss": "^3.4.17"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.7.9"
  }
}
